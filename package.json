{
  "name": "leetcode",
  "version": "1.0.1",
  "description": "My leetcode solutions.",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier \"./**/*.js\" --ignore-path ./.prettierignore --write",
    "clean": "rimraf build",
    "ts-build": "npm run clean && tsc -p ./tsconfig.json",
    "test": "jest",
    "lint": "eslint --ext .js,.vue . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gengjiawen/leetcode.git"
  },
  "author": "gengjiawen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gengjiawen/leetcode/issues"
  },
  "homepage": "https://github.com/gengjiawen/leetcode#readme",
  "dependencies": {
    "axios": "0.18.0",
    "fs-extra": "5.0.0",
    "sequelize": "4.35.5",
    "sqlite3": "3.1.13",
    "to-markdown": "3.1.1"
  },
  "jest": {
      "transform": {
          "^.+\\.tsx?$": "ts-jest"
      },
      "testMatch": [
          "**/?(*.)(spec|test).(ts|tsx|js)",
          "**/__tests__/*.(ts|tsx|js)",
          "**/test/*.(ts|tsx|js)"
      ],
      "moduleFileExtensions": [
          "ts",
          "tsx",
          "js"
      ]
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/fs-extra": "5.0.1",
    "@types/jest": "22.2.0",
    "@types/node": "9.4.7",
    "@types/rimraf": "2.0.2",
    "@types/sequelize": "^4.27.10",
    "@types/to-markdown": "3.0.0",
    "babel-core": "6.26.0",
    "babel-jest": "22.4.1",
    "eslint": "4.18.2",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.0.1",
    "jest": "22.4.2",
    "nodemon": "1.17.1",
    "prettier": "1.13.5",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.6",
    "ts-node": "6.1.1",
    "typescript": "2.9.2"
  }
}
